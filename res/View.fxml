<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="carvisualizer.controller.ViewController">
   <children>
      <Canvas fx:id="canvas" height="568.0" width="630.0" />
      <TabPane fx:id="tabPane" maxWidth="200.0" minWidth="200.0" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #DDDDDD;" tabClosingPolicy="UNAVAILABLE">
         <tabs>
            <Tab text="Filter">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefWidth="200.0">
                     <children>
                        <ScrollPane fx:id="filterScrollPane" fitToHeight="true" hbarPolicy="NEVER" prefHeight="538.0" prefWidth="200.0">
                           <content>
                              <VBox fx:id="filterVBox" spacing="5.0">
                                 <padding>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </padding>
                              </VBox>
                           </content>
                        </ScrollPane>
                     </children></AnchorPane>
               </content>
            </Tab>
            <Tab text="Interaction">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" style="-fx-background-color: #FFFFFF; -fx-border-color: #BBBBBB; -fx-border-width: 1;">
                     <children>
                        <VBox spacing="5.0">
                           <children>
                              <HBox spacing="5.0">
                                 <children>
                                    <CheckBox mnemonicParsing="false" text="Lens">
                                       <HBox.margin>
                                          <Insets top="3.0" />
                                       </HBox.margin>
                                    </CheckBox>
                                    <TitledPane animated="false" expanded="false" text="Lens Settings">
                                       <content>
                                          <VBox prefWidth="135.0" spacing="5.0">
                                             <children>
                                                <MenuButton mnemonicParsing="false" text="Attribute">
                                                  <items>
                                                    <MenuItem mnemonicParsing="false" text="Number of doors" />
                                                    <MenuItem mnemonicParsing="false" text="bore" />
                                                  </items>
                                                </MenuButton>
                                                <VBox spacing="5.0">
                                                   <padding>
                                                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                                   <children>
                                                      <HBox spacing="5.0">
                                                         <children>
                                                            <Circle fill="DODGERBLUE" radius="6.0" stroke="BLACK" strokeType="INSIDE" />
                                                            <Label text="4 doors" />
                                                         </children>
                                                      </HBox>
                                                      <HBox spacing="5.0">
                                                         <children>
                                                            <Circle fill="#ff1f1f" radius="6.0" stroke="BLACK" strokeType="INSIDE" />
                                                            <Label text="2 doors" />
                                                         </children>
                                                      </HBox>
                                                   </children>
                                                </VBox>
                                             </children>
                                          </VBox>
                                       </content>
                                    </TitledPane>
                                 </children>
                              </HBox>
                              <CheckBox fx:id="fishEyeCheckBox" mnemonicParsing="false" text="Fish Eye" />
                              <ScrollPane hbarPolicy="NEVER" prefHeight="476.0" prefWidth="187.0">
                                 <content>
                                    <VBox />
                                 </content>
                              </ScrollPane>
                           </children>
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding>
                        </VBox>
                     </children></AnchorPane>
               </content>
            </Tab>
            <Tab text="Axes">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" style="-fx-background-color: #FFFFFF; -fx-border-width: 1; -fx-border-color: #BBBBBB;">
                     <children>
                        <VBox prefHeight="539.0" prefWidth="200.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <children>
                              <MenuButton mnemonicParsing="false" text="x-axis attribute" fx:id="xAxisMenu" />
                              <MenuButton fx:id="yAxisMenu" mnemonicParsing="false" text="y-axis attribute" />
                           </children>
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding>
                        </VBox>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</HBox>
